---
kind: RoleBinding
apiVersion: rbac.authorization.k8s.io/v1beta1
metadata:
  name: graph-sa-rolebinding
  namespace: "{{ graph_namespace }}"
roleRef:
  kind: Role
  name: graph-role
  apiGroup: rbac.authorization.k8s.io
subjects:
- kind: ServiceAccount
  name: "{{ graph_sa }}"
  namespace: "{{ graph_namespace }}"
